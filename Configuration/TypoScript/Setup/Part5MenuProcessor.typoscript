page {
  10 = PAGEVIEW
  10 {
    // ...

    dataProcessing {
      # makes content elements available as {content} in Fluid template
      10 = page-content
      # provides data for the menu rendering
      20 = menu
      # provides data for the breadcrumb menu
      30 = menu
      30 {
        special = rootline
        special.range = 0|-1
        as = breadcrumb
      }
    }
  }
}
