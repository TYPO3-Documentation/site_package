{
    "name": "t3docs/site-package",
    "type": "typo3-cms-extension",
    "description": "This site package is an example used to understand the TYPO3 Site Package Tutorial.",
    "homepage": "https://docs.typo3.org/permalink/t3sitepackage:start",
    "license": [
        "GPL-2.0-or-later"
    ],
    "keywords": [
        "TYPO3 CMS"
    ],
    "require": {
        "typo3/cms-core": "^14.0",
        "typo3/cms-rte-ckeditor": "^14.0",
        "typo3/cms-fluid-styled-content": "^14.0"
    },
    "suggest": {
        "friendsoftypo3/content-blocks": "Allows to create custom content elements. "
    },
    "autoload": {
        "psr-4": {
            "T3docs\\SitePackage\\": "Classes/"
        }
    },
    "extra": {
        "typo3/cms": {
            "extension-key": "site_package"
        }
    }
}
